				</t>
			</t>
			<t t-if="o.name == 'test11'">
				<t t-set="test11" t-value="o.result" />
				<t t-if="o.test_result">
					<t t-set="test_result11">Ok</t>
				</t>
				<t t-if="not o.test_result">
					<t t-set="test_result11">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test12'">
				<t t-set="test12" t-value="o.result" />
				<t t-if="o.test_result">
