					<t t-set="test_result7">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test8'">
				<t t-set="test8" t-value="o.result" />
				<t t-if="o.test_result">
					<t t-set="test_result8">Ok</t>
				</t>
				<t t-if="not o.test_result">
					<t t-set="test_result8">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test9'">
