					<t t-set="test_result4">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test5'">
				<t t-set="test5" t-value="o.result" />
				<t t-if="o.test_result">
					<t t-set="test_result5">Ok</t>
				</t>
				<t t-if="not o.test_result">
					<t t-set="test_result5">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test6'">
				<t t-set="test6" t-value="o.result" />
				<t t-if="o.test_result">
					<t t-set="test_result6">Ok</t>
				</t>
				<t t-if="not o.test_result">
					<t t-set="test_result6">X</t>
				</t>
