		<h4 style="text-decoration: underline;"><center>Quality Report for MicroDuct OD  (20 X 16 mm one way ribbed)</center></h4>

		<t t-foreach="e.micro_duct_pipe" t-as="o">
			<t t-if="o.name == 'test1'">
				<t t-set="test1" t-value="o.result"/>
				<t t-if="o.test_result">
					<t t-set="test_result1">OK</t>
				</t>
				<t t-if="not o.test_result">
					<t t-set="test_result1">X</t>
				</t>
			</t>
			<t t-if="o.name == 'test2'">
